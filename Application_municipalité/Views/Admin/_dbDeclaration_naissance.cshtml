@model IEnumerable<Application_municipalité.Models.declaration_naissance>

@{
    ViewBag.Title = "_dbDeclaration_naissance";
}

<style>
    .he {
        margin-left: 30px;
        border-left: 10px solid #484848;
        border-right: 10px solid #484848;
        background-color: #818181;
        color: #282828;
        font-family: Arial;
        text-align: center;
        margin-bottom:20px;
    }
</style>

<h1 class="he">Declaration naissance</h1>
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.nom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.prenom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Numéro_national_didentité)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sexe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prenom_mere)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nom_mère)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prenom_pere)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date_naissance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HeurN)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MinN)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.typeN)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Communaute)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomberB)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Territoire)
            </th>
            <th></th>
        </tr>
    </thead>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.prenom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Numéro_national_didentité)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sexe)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Prenom_mere)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nom_mère)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Prenom_pere)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date_naissance)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HeurN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MinN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.typeN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Communaute)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NomberB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Territoire)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idD }) |
          
        </td>
    </tr>
}

</table>
